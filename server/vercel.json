{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/submit",
        "dest": "/api/submit"
      },
      {
        "src": "/api/download",
        "dest": "/api/download"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }