{
  "version": 2,
  "builds": [
      {
          "src": "server2.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/api/submit",
          "dest": "/api/submit",
          "methods": ["POST"]
      },
      {
          "src": "/api/download",
          "dest": "/api/download",
          "methods": ["GET"]
      },
      {
          "src": "/static/(.*)",
          "dest": "/static/$1"
      }
  ]
}