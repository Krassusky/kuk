{
    "version": 2,
    "builds": [
        {
            "src": "server/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "server/server2.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/submit",
            "dest": "/server/server.js",
            "methods": ["POST"]
        },
        {
            "src": "/api/download",
            "dest": "/api/download",
            "methods": ["GET"]
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}